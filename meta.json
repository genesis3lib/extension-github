{
  "version": "3.0.0",
  "metadata": {
    "id": "vcs-github",
    "name": "GitHub Actions CI/CD",
    "kind": "vcs",
    "layers": [
      "cicd"
    ],
    "type": "github",
    "description": "Complete GitHub Actions CI/CD workflows for infrastructure setup, deployment, and testing",
    "version": "1.0.0",
    "author": "Genesis Team",
    "tags": [
      "github",
      "cicd",
      "actions",
      "deployment",
      "automation"
    ]
  },
  "scaffoldConfigTemplate": [
    {
      "key": "githubOrg",
      "label": "GitHub Organization/User",
      "type": "select",
      "options": "runtime",
      "default": "runtime",
      "required": true,
      "description": "GitHub organization or username for repository",
      "category": "cicd"
    },
    {
      "key": "githubRepo",
      "label": "GitHub Repository Name",
      "type": "text",
      "default": "runtime",
      "required": true,
      "description": "GitHub repository name",
      "category": "cicd"
    }
  ],
  "dependencies": {},
  "generation": {
    "files": {
      "remove": []
    },
    "layers": {
      "cicd": {}
    }
  }
}